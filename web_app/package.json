{
  "name": "sudoku-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit tests/unit/*.spec.js",
    "test": "jest",
    "fix": "eslint --fix --ext .js,.vue src"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-loader": "^7.1.5",
    "base-64": "^0.1.0",
    "es6-promise": "^4.2.5",
    "eslint": "^5.9.0",
    "jest-vue-preprocessor": "^1.4.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "scrypt-js": "^2.0.4",
    "validator": "^13.7.0",
    "vue": "^2.5.17",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-meta": "^1.5.5",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.2",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-plugin-unit-mocha": "^3.1.1",
    "@vue/cli-service": "^3.1.4",
    "@vue/eslint-config-standard": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "jest": "^23.6.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "vue/attribute-hyphenation": [
        "error",
        "always"
      ],
      "vue/html-indent": [
        "error",
        2
      ],
      "vue/html-end-tags": "error"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "jest-vue-preprocessor",
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
